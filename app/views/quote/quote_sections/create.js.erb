$("#report_layout_list").append("<%= escape_javascript(render(:partial => 'quote/quote_sections/display_section', :locals => {:project=> @project, :quote => @quote, :quote_section => @quote_section})) %>");
$('#<%=dom_id(@quote_section)%> input.submit').hide();

// Show message
main_notification.display_message('New section added');

$("html, body").animate({ scrollTop: $(document).height() }, 1000);

$('#<%=dom_id(@quote_section)%> .outline_container').delay(500).effect("highlight", {}, 3000);