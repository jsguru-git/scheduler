<h1><%=@page_title = 'Admin - Accounts > Projects'%></h1>

<%=render :partial => 'shared/pagination/paginate', :object => @projects%>
<br/>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>

        <th>Created at</th>
    </tr>
    <%for project in @projects -%>
        <tr>
            <td><%=project.id%></td>
            <td><%=h project.name%></td>

            <td><%=fmt_long_date project.created_at%></td>
        </tr>
    <%end-%>
</table>
<br/>
<%=render :partial => 'shared/pagination/paginate', :object => @projects%>